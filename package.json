{
  "name": "electron-skye-browser",
  "version": "1.0.0",
  "description": "A simple browser app using skye's source code as its core",
  "main": "build/main.bundle.js",
  "scripts": {
    "start": "electron .",
    "watch": "concurrently \"npm run dev-renderer\" \"npm run dev-main\"",
    "dev-renderer": "cross-env DEV=1 webpack serve --config webpack.config-renderer.js",
    "dev-main": "cross-env DEV=1 webpack --config webpack.config-main.js",
    "build-renderer": "webpack --config webpack.config-renderer.js",
    "build": "rimraf build && concurrently \"npm run build-renderer\" \"webpack\"",
    "lint": "eslint \"src/**/*.ts*\" \"src/**/*.tsx*\"",
    "lint-fix": "npm run lint -- --fix",
    "rebuild": "electron-builder install-app-deps",
    "postinstall": "patch-package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/msynk/electron-skye-browser.git"
  },
  "keywords": [],
  "author": "msynk",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/msynk/electron-skye-browser/issues"
  },
  "homepage": "https://github.com/msynk/electron-skye-browser#readme",
